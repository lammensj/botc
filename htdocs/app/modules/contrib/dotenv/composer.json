{
    "name": "drupal/dotenv",
    "type": "drupal-module",
    "description": "Integrates the Symfony Dotenv component with Drupal",
    "license": "MIT",
    "require": {
        "drupal/core": "^10",
        "symfony/dotenv": "^5.4 || ^6.0"
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        }
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9 || ^10 || ^11"
            }
        }
    }
}
